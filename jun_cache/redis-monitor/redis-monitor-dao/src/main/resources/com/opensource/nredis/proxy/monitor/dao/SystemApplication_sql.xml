<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<!--
ISystemApplicationDao mybatis

@author liubing
@date 2016/12/31 08:09
@version v1.0.0
-->
<mapper namespace="com.opensource.nredis.proxy.monitor.dao.ISystemApplicationDao" >
    <resultMap id="SystemApplicationMap" type="com.opensource.nredis.proxy.monitor.model.SystemApplication">
        <result property="id" column="id"/>
        <result property="applicationName" column="application_name"/>
        <result property="applicationHost" column="application_host"/>
        <result property="applicationPort" column="application_port"/>
        <result property="jmxHost" column="jmx_host"/>
        <result property="jmxPort" column="jmx_port"/>
        <result property="jmxUserName" column="jmx_user_name"/>
        <result property="jmxPassWord" column="jmx_pass_word"/>
        <result property="createTime" column="create_time"/>
        <result property="createUserName" column="create_user_name"/>
        <result property="version" column="version"/>
        <result property="status" column="status"/>
        <result property="jmxStatus" column="jmx_status"/>
    </resultMap>

    <insert id="insert" parameterType="com.opensource.nredis.proxy.monitor.model.SystemApplication">
            <selectKey keyProperty="id" resultType="Integer" order="AFTER">
                SELECT LAST_INSERT_ID() AS id
            </selectKey>
        INSERT INTO system_application (
        <trim suffixOverrides=",">
                id,
                application_name,
                application_host,
                application_port,
                jmx_host,
                jmx_port,
                jmx_user_name,
                jmx_pass_word,
                create_time,
                create_user_name,
                version,
                status,
                jmx_status,
        </trim>
        )VALUES(
        <trim suffixOverrides=",">
                    #{id},
                    #{applicationName},
                    #{applicationHost},
                    #{applicationPort},
                    #{jmxHost},
                    #{jmxPort},
                    #{jmxUserName},
                    #{jmxPassWord},
                    #{createTime},
                    #{createUserName},
                    #{version},
                    #{status},
                    #{jmxStatus},
        </trim>
        )
    </insert>

    <update id="updateById" parameterType="com.opensource.nredis.proxy.monitor.model.SystemApplication">
        update system_application
        <trim prefix="SET" suffixOverrides=",">
            <if test="id != null">id = #{id},</if>
            <if test="applicationName != null">application_name = #{applicationName},</if>
            <if test="applicationHost != null">application_host = #{applicationHost},</if>
            <if test="applicationPort != null">application_port = #{applicationPort},</if>
            <if test="jmxHost != null">jmx_host = #{jmxHost},</if>
            <if test="jmxPort != null">jmx_port = #{jmxPort},</if>
            <if test="jmxUserName != null">jmx_user_name = #{jmxUserName},</if>
            <if test="jmxPassWord != null">jmx_pass_word = #{jmxPassWord},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="createUserName != null">create_user_name = #{createUserName},</if>
            <if test="version != null">version = #{version},</if>
            <if test="status != null">status = #{status},</if>
            <if test="jmxStatus != null">jmx_status = #{jmxStatus},</if>
        </trim>
        where id = #{id}
    </update>
	
	<!--乐观锁  -->
	<update id="updateVersionByIdAndVersion" parameterType="com.opensource.nredis.proxy.monitor.model.SystemApplication">	  
		update system_application set version = #{version}+1,status = #{status},jmx_status = #{jmxStatus} where id = #{id} and version = #{version}
	</update>
	
    <delete id="deleteById" parameterType="int">
        delete from system_application where id = #{id}
    </delete>

    <select id="getById" parameterType="int" resultMap="SystemApplicationMap">
        select * from system_application where id = #{id}
    </select>

    <select id="getListByCriteria" resultMap="SystemApplicationMap" parameterType="com.opensource.nredis.proxy.monitor.model.SystemApplication">
        select * from system_application
        <include refid="select_where_clause"/>
        order by id desc
    </select>
	
	<select id="getListByStatus" resultMap="SystemApplicationMap" parameterType="int">
	  select * from system_application where status=#{status}  order by id asc LIMIT 0 ,20
	</select>
	
	
    <sql id="select_where_clause">
        <trim prefix="where" prefixOverrides="AND|OR">
            <if test="id != null"> AND id = #{id} </if>
            <if test="applicationName != null"> AND application_name = #{applicationName} </if>
            <if test="applicationHost != null"> AND application_host = #{applicationHost} </if>
            <if test="applicationPort != null"> AND application_port = #{applicationPort} </if>
            <if test="jmxHost != null"> AND jmx_host = #{jmxHost} </if>
            <if test="jmxPort != null"> AND jmx_port = #{jmxPort} </if>
            <if test="jmxUserName != null"> AND jmx_user_name = #{jmxUserName} </if>
            <if test="jmxPassWord != null"> AND jmx_pass_word = #{jmxPassWord} </if>
            <if test="createTime != null"> AND create_time = #{createTime} </if>
            <if test="createUserName != null"> AND create_user_name = #{createUserName} </if>
            <if test="version != null"> AND version = #{version} </if>
            <if test="status != null">status = #{status}</if>
            <if test="jmxStatus != null">jmx_status = #{jmxStatus},</if>
             
        </trim>
    </sql>
    
    <select id="queryPageList" resultMap="SystemApplicationMap">
        select * from system_application
        <include refid="page_where_clause"/>
        order by id desc
        <if test="page!=null">
            LIMIT #{page.pageSize} OFFSET #{page.begin}
        </if>
    </select>

    <select id="count" resultType="int" parameterType="com.opensource.nredis.proxy.monitor.model.SystemApplication">
        select count(*) from system_application
        <include refid="page_where_clause"/>
    </select>

    <sql id="page_where_clause">
        <trim prefix="where" prefixOverrides="AND|OR">
            <if test="queryObject.id != null"> AND id = #{queryObject.id} </if>
            <if test="queryObject.applicationName != null"> AND application_name = #{queryObject.applicationName} </if>
            <if test="queryObject.applicationHost != null"> AND application_host = #{queryObject.applicationHost} </if>
            <if test="queryObject.applicationPort != null"> AND application_port = #{queryObject.applicationPort} </if>
            <if test="queryObject.jmxHost != null"> AND jmx_host = #{queryObject.jmxHost} </if>
            <if test="queryObject.jmxPort != null"> AND jmx_port = #{queryObject.jmxPort} </if>
            <if test="queryObject.jmxUserName != null"> AND jmx_user_name = #{queryObject.jmxUserName} </if>
            <if test="queryObject.jmxPassWord != null"> AND jmx_pass_word = #{queryObject.jmxPassWord} </if>
            <if test="queryObject.createTime != null"> AND create_time = #{queryObject.createTime} </if>
            <if test="queryObject.createUserName != null"> AND create_user_name = #{queryObject.createUserName} </if>
            <if test="queryObject.version != null"> AND version = #{queryObject.version} </if>
            <if test="queryObject.status != null">status = #{queryObject.status}</if>
            <if test="queryObject.jmxStatus != null">jmx_status = #{queryObject.jmxStatus},</if>

           
        </trim>
    </sql>

    <!--extension script-->
</mapper>