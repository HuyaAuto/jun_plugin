<p class="add-defect-p add-defect-p2">
	<%for(var i=0;i<checkBoxs.length;i++){
		var checked = defaultValues.length>0?($.inArray(checkBoxs[i].value,defaultValues) > -1?true:false):false;
	%>
		<input id="checkBox<%=(i+1)%>" class="checkbox-class"    type="checkBox" value="<%=checkBoxs[i].value%>" save="<%=checked%>" <%=checked?'checked':''%>>
    	<label id="label<%=(i+1)%>"   class="checkbox-label-class <%=checked?'checkbox-selected':''%>"><%=checkBoxs[i].name%></label>
	<%}%>
</p>