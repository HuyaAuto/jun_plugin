template header
field
lower
upper
codes

package org.drools.range;
template "inRange"
rule "is in range @{row.rowNumber}"
when
    $item : Item( $name : name, $v : @{field} >= @{lower} && <= @{upper}, $code : code @{codes} )
then
    $item.setOutput ("Item " + $name + " @{field} in range: " + $v + " code: " + $code);
    System.out.println( $item.getOutput() );
end
end template